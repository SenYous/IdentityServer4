  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/{url}", //服务地址--URL变量
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5726 //服务端口
